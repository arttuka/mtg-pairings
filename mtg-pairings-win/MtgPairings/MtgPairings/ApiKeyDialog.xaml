<Window x:Class="MtgPairings.ApiKeyDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MtgPairings"
        mc:Ignorable="d"
        Title="Syötä API key" Height="120" Width="380"
        ResizeMode="NoResize">
    <Grid>
        <Label x:Name="label" Content="Syötä API key. Löydät sen osoitteesta http://mtgsuomi.fi/apikey." HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,0,0,0"/>
        <TextBox x:Name="ApiKeyTextbox" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="347" Margin="10,26,0,0"/>
        <Button x:Name="OkButton" Content="OK" HorizontalAlignment="Left" Margin="282,54,0,0" VerticalAlignment="Top" Width="75" Click="OkButton_Click"/>

    </Grid>
</Window>
